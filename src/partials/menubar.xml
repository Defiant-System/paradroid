<MenuBar>
	<Menu name="Paradroid">
		<Menu name="About Paradroid" shell="win -a"/>
		<Menu type="divider"/>
		<Menu name="Quit Paradroid" shell="win -c"/>
	</Menu>
	<Menu name="Game" for="game-content">
		<Menu name="Show&#8230;">
			<Menu name="Start" click="show-view" arg="start"/>
			<Menu name="Lift" click="show-view" arg="lift"/>
			<Menu name="Mobile" click="show-view" arg="mobile"/>
			<Menu name="Transfer" click="show-view" arg="transfer"/>
			<Menu name="Console" click="show-view" arg="console"/>
			<Menu name="Editor" click="show-view" arg="editor"/>
		</Menu>
		<Menu type="divider"/>
		<Menu name="Editor" for="editor-content">
			<Menu name="Toggle Level BG" click="toggle-bg"/>
			<Menu name="Toggle Grid" click="toggle-grid"/>
			<Menu type="divider"/>
			<Menu name="Level&#8230;">
				<Menu name="a" check-group="game-level" click="render-level" arg="a" is-checked="1"/>
				<Menu name="b" check-group="game-level" click="render-level" arg="b"/>
				<Menu name="c" check-group="game-level" click="render-level" arg="c"/>
				<Menu name="d" check-group="game-level" click="render-level" arg="d"/>
				<Menu name="e" check-group="game-level" click="render-level" arg="e"/>
				<Menu name="f" check-group="game-level" click="render-level" arg="f"/>
				<Menu name="g" check-group="game-level" click="render-level" arg="g"/>
				<Menu name="h" check-group="game-level" click="render-level" arg="h"/>
				<Menu name="i" check-group="game-level" click="render-level" arg="i"/>
				<Menu name="j" check-group="game-level" click="render-level" arg="j"/>
				<Menu name="k" check-group="game-level" click="render-level" arg="k"/>
				<Menu name="l" check-group="game-level" click="render-level" arg="l"/>
				<Menu name="m" check-group="game-level" click="render-level" arg="m"/>
				<Menu name="n" check-group="game-level" click="render-level" arg="n"/>
				<Menu name="o" check-group="game-level" click="render-level" arg="o"/>
				<Menu name="p" check-group="game-level" click="render-level" arg="p"/>
				<Menu name="q" check-group="game-level" click="render-level" arg="q"/>
				<Menu name="r" check-group="game-level" click="render-level" arg="r"/>
				<Menu name="s" check-group="game-level" click="render-level" arg="s"/>
				<Menu name="t" check-group="game-level" click="render-level" arg="t"/>
			</Menu>
			<Menu type="divider"/>
			<Menu name="Output PGN" click="output-pgn"/>
		</Menu>
	</Menu>
	<Menu name="Help">
		<Menu name="Help" click="open-help"/>
		<Menu type="divider"/>
		<Menu name="License" shell="win -al"/>
	</Menu>
</MenuBar>