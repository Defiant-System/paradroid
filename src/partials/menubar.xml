<MenuBar>
	<Menu name="Paradroid">
		<Menu name="About Paradroid" shell="win -a"/>
		<Menu type="divider"/>
		<Menu name="Quit Paradroid" shell="win -c"/>
	</Menu>
	<Menu name="Game" for="game-content">
		<Menu name="Show&#8230;">
			<Menu name="Start" click="show-view" arg="start"/>
			<Menu name="Lift" click="show-view" arg="lift"/>
			<Menu name="More" click="show-view" arg="more"/>
			<Menu name="Mobile" click="show-view" arg="mobile"/>
			<Menu name="Transfer" click="show-view" arg="transfer"/>
			<Menu name="Console" click="show-view" arg="console"/>
			<Menu type="divider"/>
			<Menu name="Editor" click="show-view" arg="editor"/>
		</Menu>
		<Menu type="divider"/>
		<Menu name="Debug Mode">
			<Menu name="0" check-group="Debug" click="set-debug-mode" arg="0" is-checked="1"/>
			<Menu name="1" check-group="Debug" click="set-debug-mode" arg="1"/>
			<Menu name="2" check-group="Debug" click="set-debug-mode" arg="2"/>
		</Menu>
		<Menu type="divider"/>
		<Menu name="Editor" for="editor-content">
			<Menu name="Toggle Grid" click="toggle-grid"/>
			<Menu type="divider"/>
			<Menu name="Section&#8230;">
				<Menu name="1" check-group="game-level" click="render-level" arg="1" is-checked="1"/>
				<Menu name="2" check-group="game-level" click="render-level" arg="2"/>
				<Menu name="3" check-group="game-level" click="render-level" arg="3"/>
				<Menu name="4" check-group="game-level" click="render-level" arg="4"/>
				<Menu name="5" check-group="game-level" click="render-level" arg="5"/>
				<Menu name="6" check-group="game-level" click="render-level" arg="6"/>
				<Menu name="7" check-group="game-level" click="render-level" arg="7"/>
				<Menu name="8" check-group="game-level" click="render-level" arg="8"/>
				<Menu name="9" check-group="game-level" click="render-level" arg="9"/>
				<Menu name="10" check-group="game-level" click="render-level" arg="10"/>
				<Menu name="11" check-group="game-level" click="render-level" arg="11"/>
				<Menu name="12" check-group="game-level" click="render-level" arg="12"/>
				<Menu name="13" check-group="game-level" click="render-level" arg="13"/>
				<Menu name="14" check-group="game-level" click="render-level" arg="14"/>
				<Menu name="15" check-group="game-level" click="render-level" arg="15"/>
				<Menu name="16" check-group="game-level" click="render-level" arg="16"/>
				<Menu name="17" check-group="game-level" click="render-level" arg="17"/>
				<Menu name="18" check-group="game-level" click="render-level" arg="18"/>
				<Menu name="19" check-group="game-level" click="render-level" arg="19"/>
				<Menu name="20" check-group="game-level" click="render-level" arg="20"/>
			</Menu>
			<Menu type="divider"/>
			<Menu name="Output PGN" click="output-pgn"/>
		</Menu>
	</Menu>
	<Menu name="Help">
		<Menu name="Help" click="open-help"/>
		<Menu type="divider"/>
		<Menu name="License" shell="win -al"/>
	</Menu>
</MenuBar>